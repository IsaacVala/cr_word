{
  "name": "office-addin-taskpane",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "engines": {
    "node": ">=16 <19",
    "npm": ">=7 <10"
  },
  "scripts": {
    "dev-server": "node ./bin/www",
    "start": "office-addin-debugging start manifest.json --app outlook",
    "stop": "office-addin-debugging stop manifest.json",
    "validate": "office-addin-manifest validate manifest.json"
  },
  "dependencies": {
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "office-addin-debugging": "^5.0.4",
    "office-addin-dev-certs": "^1.11.2",
    "office-addin-manifest": "^1.12.3"
  },
  "browserslist": [
    "ie 11"
  ]
}